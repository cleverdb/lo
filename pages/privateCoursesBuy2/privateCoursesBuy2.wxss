/* pages/privateCoursesBuy2/privateCoursesBuy2.wxss *//* pages/privateCoursesBuy/privateCoursesBuy.wxss */
@import '../template/pageAlert/pageAlert.wxss';
page{
   height: 100%;
}
.try_course_container{
    /* height: 100vh; */
    background-color: #ffffff;
    padding-bottom: 112rpx;
}
.icon_down{
    margin-left: 20rpx;
    width: 26rpx;
    height: 16rpx;
}
.buy_button{
    display: flex;
    justify-content: center;
    align-items: center;
    background-color:#FCC800;
    height:110rpx;
    border-radius:0;
    font-size: 30rpx;
    color: #3F3B3A;
    position: fixed;
    bottom: 0;
    width: 100%;
}
.content{
    padding-left: 30rpx;
    padding-right: 30rpx;
}
.head{
    display: flex;
    /* height: 401rpx; */
}
.head image{
    height:401rpx;
    flex: 1;
}
.content_item{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    padding-top:40rpx;
    padding-bottom:40rpx;
    border-bottom: 1rpx  solid #E4E5E5;
}
.content_item_contents{
     display: flex;
    flex-direction: row;
    align-items: center;
}
.content_item .arrow{
    padding-right: 16px;
    position: relative;
}
.arrow::after {
  content: " ";
  display: inline-block;
  height: 12rpx;
  width: 12rpx;
  border-width: 4rpx 4rpx 0 0;
  border-color: #9FA0A0;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  /* position: relative; */
  position: absolute;
  top: 30rpx;
  margin-top: -8rpx;
  right: 4rpx;
}
.content_item  .label{
  color: #9FA0A0;
}
.content_item .value{
    font-size: 28rpx;
}
.content_item text{
    font-size: 27rpx;
    font-weight:400;
    color:rgba(0,0,0,1);
}
.color_red text{
    color: #E7355E;
}
.color_gary text{
     color:  #9E9F9F;
}
.notic text{
 font-size: 20rpx;
 color:#727272;

}
.notice{
    margin-top: 30rpx;
}
.notice text{
   
    color: #3F3B3A;
}
.read{
    margin-bottom: 40rpx;
    padding-left:0 ;
    display: flex;
    align-items: center;
}
radio .wx-radio-input{
height: 22rpx;
width: 22rpx;
}
.read radio{
    display: flex;
    justify-content: center;
    align-items: center;
}
.read text{
    font-size: 22rpx;
    color: #3F3B3A;
    display: block;
}
.content_item .hot{
    margin-top: 2rpx;
    margin: 0 15rpx;
    margin-top: -5rpx;
    /* font-size: 13px !important; */
    padding: 5rpx 6rpx 6rpx 8rpx;
    font-size: 14rpx;
    background-color: #FCC800;
}
.read .toEnsure{
   color: #43B9AA;
}
.teachersListCantainer{
    padding: 20rpx 30rpx 0;
    background-color: #EFEFEF;
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
}
.teacherslist_item{
    position: relative;
    display: inline-block;
    width: 336rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
    margin-bottom: 20rpx;
   

}
.course_img{ 
  height:253rpx;
  width: 336rpx;
  background: #DCDDDD;
  overflow: hidden;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  /* padding:; */
}
.course_no_img{
    display: flex;
  justify-content: center;
  align-items: center;
}
.course_img image{
    width: 100%;
}
.footer_bar{
  background-color: white;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  align-items:center;
  padding-left: 15rpx;
  padding-right: 17rpx;
  padding-top:20rpx;
  padding-bottom:20rpx;

}
.tag{
  line-height:inherit;
  padding:2rpx 13rpx;
  background-color: white;
  color: #A7A7A8;
  display: inline-block;
  margin: 0;
  font-size: 18rpx;
  margin-left: 10rpx;
}
.course_name{
  font-size: 30rpx;
}
.course_name_right{
    display: flex;
}
.topborder{
    border-top: 1rpx  solid #E4E5E5;
}
.bordernone{
    border:none;
}
.besel_icon{
    width:68rpx;
    height:68rpx;
    position: absolute;
    right: 0;
    top: 0;
}
.numContainer{
    display: flex;
    border: 1rpx solid #B5B6B6;
    border-radius:4rpx;
    overflow: hidden;
}
.numContainer text{
    font-size:30rpx;
    
}
.numleft,.numright,.numcontent {
    width:74rpx;
    height:59rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.numright{
    background-color:#FCC800;
}
.numleft,.numright{
   border: none;

}
.numcontent{
    border-left: 1rpx solid #B5B6B6;
    border-right: 1rpx solid #B5B6B6;
}
.footer{
    position: fixed;
    width: 100%;
    bottom: 0;
}
.agree_content{
    width:239rpx;
    height:40rpx;
    background:#E7365F;
    text-align: center;
    line-height: 40rpx;
    font-size:20rpx;
    color: #ffffff;
    border-radius: 8rpx;
    margin: 8rpx 0;
}
.agree_content_checked{
    background: #ffffff;
}

.popup-container {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #090000;
  opacity: 0.2;
  z-index: 500;
  color: #fff;
}
.popup-content {
  height: 600rpx;
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 600;
  color: #a0a0a0;
  background-color: #EFEFEF;
  padding-top: 20rpx;
}
.popup-content-bd {
  width: 100%;
  height: 600rpx;
  position: relative;
}

.content-wrapper {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

 .sv {
  height: 600rpx;
  padding-bottom: 20rpx;

}

.no-ticket {
  width: 100%;
  text-align: center;
  padding-top:100rpx;
}

.no-ticket-img {
  width: 120rpx;
  height: 120rpx;
  margin-bottom: 30rpx;
}

.no-ticket .text {
  font-size: 28rpx;
  color: #a0a0a0;
  text-align: center, ;
}

.ticket-view {
  position: relative;
  height: 180rpx;
  display: flex;
  flex-direction: row;
  margin: 20rpx;
  color: #3e3e3e;
  overflow: hidden;
  border-radius: 10rpx;
}

.ticket-view .ticket-view-left {
  width: 70%;
  display: flex;
  flex-direction: column;
  padding: 0 30rpx;
  background-color: #ffffff;
  border-top-right-radius: 17rpx;
  border-bottom-right-radius: 17rpx;
}
.ticket-view-center{
  padding: 10rpx 0;
  width: 1px;
  display: flex;
}
.ticket-view-center > view{
  width: 1px;
  border-right: 2rpx #ABACAC dashed;
}
.ticket-view-left .hd {
  height: 70%;
  display: flex;
  align-items: center;
  font-size: 34rpx;
  font-weight: 900;
  border-bottom: 2rpx #3e3e3e solid;
}

.ticket-view-left .bd {
  height: 30%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
}

.ticket-view .ticket-view-right {
  width: 30%;
  background-color: #ffffff;
  border-top-left-radius: 17rpx;
  border-bottom-left-radius: 17rpx;
}

.ticket-view-right .hd {
  height: 70%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.ticket-view-right .hd .num {
  font-size: 48rpx;
  align-self: center;
  font-weight: 900;
}
.ticket-view .usable{
  color: #FCC700;
}
.ticket-view .disabled{
  color: #9E9F9F
}
.ticket-view-right .hd .logo {
  margin-bottom: 1rpx;
  align-self: center;
  font-size: 28rpx;
  /* font-weight: 700; */
}
.ticket-view-right .bd {
  font-size: 28rpx;
  text-align: center;
}

.select{
  border:1px solid #fcc700;
}
.usable_title{
  font-size:28rpx;
  padding-left: 30rpx;
}
.unusable_title{
  font-size:20rpx;
  padding-left: 30rpx;
}
.unusable_footer{
  height: 20rpx;
}
.usable_footer{
  height: 1px;
}
.threshold_reason{
  color: red;
  font-size: 20rpx;
  padding-left: 20rpx;
  margin-top: 10rpx;
}
.unusable_view{
  margin-bottom: 0;
}
