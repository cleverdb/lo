/*工作，最外层容器*/
.work_container{
  width: 100%;
  height: 100%;
  overflow: auto;
  background-color: #EEEFEF;
  font-size: 28rpx;
  color: #3E3A39;
  font-family: PingFang-SC-Bold;
}

.card_container0{
  width: 100%;
  height: auto;
  background-color: #ffffff;
  
}
.margin_bottom_19{
  margin-bottom: 19rpx;
}

.card_title{
  width: 100%;
  height: 80rpx;
  padding-left: 30rpx;
  display: flex;
  align-items: center;
  font-weight: bold;
  border-bottom: 2rpx solid #F5F5F5;
}
.card_title_img{
  width: 4rpx;
  height: 32rpx;
  margin-right: 7rpx;
}

.card_content{
  margin: 0 30rpx;
  width: cal(100%-60rpx);
  height: auto;
  background: black;
}
.i_cell_height{
  height: 104rpx;
}
.card_item{
  display: flex;
  align-items: center;
  position: relative;
}
.card_item_1_img{
  width: 59rpx;
  height: 51rpx;
  margin-right: 15rpx;
}

.card_item_note{
  color: #9FA0A0;
  font-size: 24rpx;
  position: absolute;
  right: 20rpx;
}