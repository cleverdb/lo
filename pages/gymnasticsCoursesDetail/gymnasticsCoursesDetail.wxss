/* pages/gymnasticsCoursesDetail/gymnasticsCoursesDetail.wxss */
.container view{
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
}
.container{
    background-color: #ffffff;
    min-height: 100vh;
}
.left{
    padding: 20rpx;
}
.right{
    padding: 20rpx;
}
.container .dateSel{
    align-items: center;
    justify-content: center;
    display: flex;
    flex-direction: row;
}
.container .table_head{
    flex-direction:row;
}
.container .table_head view{
    font-size: 22rpx;
    height: 65rpx;
    flex:1 0 auto;
    width:176rpx;
    justify-content: center;
    align-items: center;
    border:1rpx solid #E5E5E5;
    border-right: none;
    border-bottom: none;
}
.container .table_head view:last-child{
   border-right:1rpx solid #E5E5E5;
}
.container .body{
    flex-direction: row;
}
.container .body>view{
    border-top:1rpx solid #E5E5E5;
}
.container .body>view:last-child>view{
    border-right:1rpx solid #E5E5E5;
}
.container .body .body_item{
    line-height:27rpx;
    font-size:20rpx;
    /* padding:22rpx 28rpx; */
    flex:1 0 auto;
    border:1rpx solid #E5E5E5;
    border-right:none;
    border-top:none;
    width:176rpx;
    height:116rpx;
}
.container .body .body_item text{
    padding-top:22rpx;
    padding-bottom: 22rpx;
    padding-left: 28rpx;
    padding-right: 28rpx;
}
.container  ._table{
    flex-direction: row;
}
.container  ._table_col{
    flex:1 0 auto;
    width: 176rpx;
    border:1rpx solid  #E4E5E5;
    border-right:none;
}
.container  ._table_head{
    border-bottom:1rpx solid  #E4E5E5; 
}
.container .body_col{
    height:116rpx;
    overflow-y: auto;
    flex: 1 0 auto;
    /* border-top:1rpx solid  #E4E5E5; */
    border-bottom:1rpx solid  #E4E5E5;

}
.container .tr_col{
    box-sizing:border-box;
    padding:22rpx 28rpx;
    height:116rpx;
    width: 176rpx;
    justify-content: center;
    align-items: center;

    /* border-top:1rpx solid  #E4E5E5; */
    margin-bottom: 10rpx;
}
.container .tr_col text{
    font-weight:bold;
    color:rgba(0,0,0,1);
    font-size: 20rpx;
}
.container .body_col .tr_col:last-child{
    /* border-top:1rpx solid  #E4E5E5; */
    margin-bottom:0;
}
.container  ._table_head{
    background-color: #EEEFEF;
    justify-content: center;
    height: 65rpx;
    width: 173rpx;
    align-items: center;
}
.container  ._table_head text{
    font-size:22rpx;
}
._card{
   padding:0 30rpx; 
}
._card ._card_head{
    border-bottom:1rpx solid #E4E4E5;
    padding-top:35rpx;
    padding-bottom:30rpx;
    flex-direction:row;
    align-items: flex-end;
}
._card ._card_head_title{
    font-size:28rpx;
    color:rgba(0,0,0,1);
    font-weight:400;
}
._card ._card_head_subheading{
    margin-left: 15rpx;
    margin-right: 20rpx;
    font-size: 20rpx;
    color: #BBBBBC;
}
._card ._card_head_details{
    padding-top:24rpx;
    padding-bottom: 54rpx;
    font-size: 20rpx;
    color: #BBBBBC;
}
._card  ._card_head_mark{
    flex-direction: row;
    align-items: baseline;
}

._card  ._card_head_mark>text:nth-child(2){
    font-size: 20rpx;
    
}
._card  ._card_head_mark>text:nth-child(1){
    font-size: 28rpx;
}
._card ._card_head_details>text{
    line-height: 1.8;
}
.space{
    width: 100%;
    height: 20rpx;
    background-color:#EEEEEF;
}
.container .person_details{
    flex-direction:row;
}
.person_details ._person_details_img{
    flex:1 0 auto ;
    width: 185rpx;
    height: 205rpx;
    margin-right:22rpx;
    border-radius:10rpx;
}
.container .person_details{
    padding-top: 36rpx;
    padding-left: 30rpx;
    padding-bottom: 24rpx;
    padding-right: 30rpx;
}
.person_details ._personDetail_name{
    font-size: 32rpx;
    font-weight:500;
}
.person_details ._personDetail_goods{
    font-size: 20rpx;
    font-weight:400;
    color: #727171;

}
.person_details ._personDetail_detail{
    font-size:20rpx;
    font-weight:400;
    line-height: 1.8;
    color: #BBBBBC;
}
.order_button {
    display: flex;
    justify-content: center;
    align-items: center;
    background-color:#FCC800;
    height:107rpx;
    border-radius:0;
    font-size: 30rpx;
    color: #3F3B3A;
    width: 100%;
}
.moveright{
    width: 39rpx;
    height: 71rpx;
    position: absolute;
    right:37rpx;
    top:179rpx;
}