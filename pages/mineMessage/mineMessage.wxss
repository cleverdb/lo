/* pages/mineMessage/mineMessage.wxss */
page{
    height: 100%;
}
.mine_message_warp{
    height: 100%;
    background: #FFFEFF;
    padding: 0 30rpx;
}
.img_warp{
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 45rpx;
    margin-bottom: 40rpx;
}
.mine_message_img{
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
    background: red;
}
.mine_photo{
    width: 74rpx;
    height: 53rpx;
}
.phote_warp{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.img_conter{
    display: flex;
    justify-content: space-between;
    padding: 0 120rpx;
}
.img_text{
    color:rgba(62,58,57,1);
    font-size: 20rpx;
    margin-top: 6rpx;
}
.mine_input{
    text-align: right;
}
.mine_panel_warp{
    border-top: 1px solid #ABABAB;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.mine_panel_title{
    font-size: 26rpx;
    color:rgba(62,58,57,1);
}
.mine_footer_warp{
    margin-top: 50rpx;
}